<!DOCTYPE html>

<html>
<head>
  
  <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
  <%-include("partials/headImports") -%>
  <style>
    .desc {
        max-width: .2vw !important;
        white-space: nowrap !important;
        text-overflow: ellipsis !important;
        overflow: hidden !important;
    }
  </style>
  
  <script src="/javascripts/curso.js"></script>

</head>

<body class="hold-transition skin-blue sidebar-mini">
    
<div class="wrapper">

  <!-- Main Header -->
  <header class="main-header">
      <%-include("partials/header") -%>
  </header>
  <!-- Left side column. contains the logo and sidebar -->
  <aside class="main-sidebar">
    <%-include("partials/sidebar",{docente: query.docente}) -%>
  </aside>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      
      <h1 >        
          <span ><i class="fas fa-chalkboard"></i><%= result.materia.sigla%> G<%= result.grupo%></span>  
          <small><%= result.materia.nombre%>, Gestion <%= result.gestion%></small>
      </h1>
      <ol class="breadcrumb">
          <li><a href="/home?docente=<%= query.docente%>"><i class="fas fa-home"></i> Inicio</a></li>
        <li><a href="/cursos?docente=<%= query.docente%>"><i class="fas fa-chalkboard"></i> Cursos</a></li>
        <li class="active"><%= result.materia.sigla%> G<%= result.grupo%></li>
      </ol>
    </section>
    
    <!-- Main content -->
    <section class="content container-fluid">
      <div class="row">
                    
        <div class="col-xs-12">
          <div class="box box-primary box-solid collapsed-box">
            <div class="box-header with-border">
              
              <h2 class="box-title text-bold "><i class="fas fa-users"></i> ESTUDIANTES </h2>

              <div class="box-tools pull-right">                  
                <button type="button" id="estudiantes" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-plus"></i>
                </button>
              </div>            
              <!-- /.box-tools -->
            </div>
            <!-- /.box-header -->
            <div class="box-body" >
              <div class="box">
                <div class="box-header"> 
                  
                  <div class="box-tools pull-right">                         
                    <div class="input-group input-group-sm" style="width: 45vw;">
                      <div class="input-group-btn">
                        <button type="button" class="btn btn-warning dropdown-toggle" data-toggle="dropdown" aria-expanded="true">Criterio
                          <span class="fa fa-caret-down"></span></button>
                        <ul class="dropdown-menu">
                          <li><a href="#">Nombres y Apellidos</a></li>
                          <li><a href="#">R. U.</a></li>
                          <li><a href="#">C. I.</a></li>
                        </ul>
                      </div>
                      <input type="text" name="table_search" class="form-control pull-right" placeholder="Buscar Estudiante">
    
                      <div class="input-group-btn">                            
                        <button type="submit" class="btn btn-warning"><i class="fas fa-search"></i></button>
                      </div>
                      
                    </div>
                  </div>
                </div> 
                <!-- /.box-header -->
                <div class="box-body">
                  <table id="example2" class="table table-bordered table-hover table-striped">
                    <thead>
                    <tr>
                      <th>Nombres y Apellidos</th>
                      <th>CI</th>
                      <th>RU</th>
                      <th>Acciones</th>
                    </tr>
                    </thead>
                    <tbody id="tEstudiantes" style="display: none">
                                                  
                    </tbody> 
                  </table>
                </div>
                <!-- /.box-body -->
                </div>
                <!-- /.box -->
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
          <div class="box box-success box-solid collapsed-box">
            <div class="box-header with-border">
              
              <h2 class="box-title text-bold "> <i class="fas fa-book"></i> GUIAS</h2>

              <div class="box-tools pull-right">                  
                <button type="button" id="guias"  class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-plus"></i>
                </button>
              </div> 
                                
              
                                
              <!-- /.box-tools -->
            </div>
            <!-- /.box-header -->
            <div class="box-body" >
                <div class="box">
                  <div class="box-header"> 
                      
                    <div class="box-tools pull-right"> 
                        
                      <div class="input-group input-group-sm" style="width: 45vw;">
                        <div class="input-group-btn ">
                          <button type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown" aria-expanded="true">Criterio
                            <span class="fa fa-caret-down"></span></button>
                          <ul class="dropdown-menu">
                            <li><a href="#">Numero</a></li>
                            <li><a href="#">Descripcion</a></li>
                            <li><a href="#">Otro Campo</a></li>
                          </ul>
                        </div>
                        <input type="text" name="table_search" class="form-control pull-right" placeholder="Buscar Guia">
      
                        <div class="input-group-btn">
                            
                          <button type="submit" class="btn btn-success"><i class="fas fa-search"></i></button>
                        </div>                            
                      </div>
                    </div>
                  </div> 
                  <!-- /.box-header -->
                  <div class="box-body">
                    <table id="example2" class="table table-bordered table-hover table-striped">
                      <thead>
                      <tr>
                        <th>NÂ° GUIA </th>
                        <th>DESCRIPCION</th>
                        <th>FECHA</th>
                        
                        <th>ACCIONES</th>
                      </tr>
                      </thead>
                      <tbody id="tGuias" style="display: none">                       
                      </tbody> 
                    </table>
                  </div>
                  <!-- /.box-body -->
                  </div>
                  <!-- /.box -->
            </div>
            <!-- /.box-body -->
          </div>
          <div class="box box-info box-solid collapsed-box">
            <div class="box-header with-border ">
              
              <h2 class="box-title text-bold "><i class="fas fa-file"></i> ARCHIVOS </i></h2>

              <div class="box-tools pull-right">                  
                <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-plus"></i>
                </button>
              </div> 
                                    
              <!-- /.box-tools -->
            </div>
            <!-- /.box-header -->
            <div class="box-body" >
                <div class="box">
                  <div class="box-header"> 
                      
                    <div class="box-tools pull-right">
                      <div class="input-group input-group-sm" style="width: 45vw;">
                        <div class="input-group-btn ">
                          <button type="button" class="btn btn-info dropdown-toggle" data-toggle="dropdown" aria-expanded="true">Criterio
                            <span class="fa fa-caret-down"></span></button>
                          <ul class="dropdown-menu">
                            <li><a href="#">Nombre</a></li>
                            <li><a href="#">Apellido</a></li>
                            <li><a href="#">R. U.</a></li>
                            
                            <li><a href="#">C. I.</a></li>
                          </ul>
                        </div>
                        <input type="text" name="table_search" class="form-control pull-right" placeholder="Buscar Estudiante">
      
                        <div class="input-group-btn">
                            
                          <button type="submit" class="btn btn-info"><i class="fas fa-search"></i></button>
                        </div>
                        
                      </div>
                    </div>
                  </div> 
                  <!-- /.box-header -->
                  <div class="box-body">
                    <table id="example2" class="table table-bordered table-hover table-striped">
                      <thead>
                      <tr>
                        <th>NOMBRE ARCHIVO</th>
                        <th>DESCRIPCION</th>
                        <th>FECHA</th>
                        <th>OTRO CAMPO</th>
                        <th>ACCIONES</th>
                      </tr>
                      </thead>
                      <tbody>
                        <%for(var i = 1; i< 10;i++){%>
                        <tr>
                          <td>Archivo <%=i%>.ext<%=i%></td>
                          <td>Descripcion archivo <%=i%></td>
                          <td>0<%=i%>/0<%=i%>/2019</td>
                          <td>Detalle Campo <%=i%></td>
                          <td>                            
                            <button type="button" class="btn  btn-primary btn-sm">
                              <i class="fas fa-info" ></i></button>
                              <button type="button" class="btn  btn-success  btn-sm">
                                  <i class="fas fa-edit" ></i>
                              </button>
                              <button type="button" class="btn  btn-danger  btn-sm">
                                  <i class="fas fa-trash" ></i>
                              </button>
                          </td>
                        </tr>   
                        <%}%>                            
                      </tbody> 
                    </table>
                  </div>
                  <!-- /.box-body -->
                  </div>
                  <!-- /.box -->
            </div>
            <!-- /.box-body -->
          </div>
        </div>
        <!-- /.col -->
        <div class="col-xs-12 col-sm-12 col-md-5 col-lg-4">
          <!-- DONUT CHART -->
          <div class="box box-warning box-solid">
              <div class="box-header with-border">
                <h3 class="box-title ">Promedio Laboratorios revisados</h3>

              </div>
              <div class="box-body">
                <canvas id="pieChart" style="height:250px;width: 250px"></canvas>
              </div>
              <!-- /.box-body -->
          </div>
          <!-- /.box -->
          </div>
        <div class="col-xs-12 col-sm-12 col-md-7 col-lg-8">
          <!-- BAR CHART -->
          <div class="box box-danger box-solid">
            <div class="box-header with-border">
              <h3 class="box-title">Detalle revision por laboratorio </h3>
            </div>
            <div class="box-body">
              <div class="chart">
                <canvas id="barChart" style="height:300px;width: 300px"></canvas>
              </div>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>



      </div>

      
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  
  <%-include("partials/footer") -%>
  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">

  </aside>
  <!-- /.control-sidebar -->
  <!-- Add the sidebar's background. This div must be placed
  immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>


</div>

</body>
</html>